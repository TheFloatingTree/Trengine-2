    "dotenv": "^10.0.0",
    "electron-debug": "^3.2.0",
    "electron-squirrel-startup": "^1.0.0",
    "howler": "^2.2.1",
    "matter-js": "^0.16.1",
    "pixi.js": "^6.1.0",
